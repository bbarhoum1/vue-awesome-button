{
  "name": "vue-awsome-button",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbarhoum1/vue-awesome-button.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue-awesome-button.umd.js",
